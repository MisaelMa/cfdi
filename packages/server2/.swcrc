{
    "sourceMaps": true,
    "module": {
      "type": "commonjs",
      "strict": true
    },
    "jsc": {
      "target": "es2017",
      "parser": {
        "syntax": "typescript",
        "decorators": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "keepClassNames": true
    }
  }